<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ProgressView">
        <attr name="cursorColor" format="color" />
        <attr name="cursorProgress" format="float" />
    </declare-styleable>

    <declare-styleable name="ProgressBarView">
        <attr name="indicator_width" format="dimension" />
        <attr name="indicator_color" format="color" />
        <attr name="anim_enabled" format="boolean" />
        <attr name="bar_type" format="enum">
            <enum name="back_and_forward" value="0" />
            <enum name="forward" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="RoundedLinearLayout">
        <attr name="stroke_width" format="dimension" />
        <attr name="stroke_color" format="color" />
        <attr name="background_color" format="color" />
    </declare-styleable>

</resources>